msgstr "Задержка в секундах перед выполнением снимка экрана"

#: ../lib/screenshooter-dialogs.c:877
msgid "seconds"
msgstr "с"

#: ../lib/screenshooter-dialogs.c:923
msgid "_Back"
msgstr "_Назад"

#: ../lib/screenshooter-dialogs.c:938
msgid "Choose what to do with the screenshot"
msgstr "Выберите, что делать со снимком экрана"

#: ../lib/screenshooter-dialogs.c:974
msgid "<span weight=\"bold\" stretch=\"semiexpanded\">Action</span>"
msgstr "<span weight=\"bold\" stretch=\"semiexpanded\">Действие</span>"

